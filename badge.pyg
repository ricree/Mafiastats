DEFINITION -> DEF ENDLINE
	| DEF
	;
DEF -> LINE DEF
	| LINK
	;
LINE -> ENDLINE NEWLINE
	;
ENDLINE -> ENDLINE ENDLINE
	| STRINGP
	| TOKEN
	;

TOKEN -> FIELD DESCRIPTION
	| FIELD
	;

DESCRIPTION -> DECORATION SIZE
	| SIZE DECORATION
	| SIZE
	| DECORATOR
	;

FIELD -> WINS
	| NAME
	| MODERATED
	| LOSSES
	| TOTAL
	;
DECORATION-> BOLD
	| ITALIC
	| NORMAL
	;

STRINGP -> OPEN STRING CLOSE DESCRIPTION
	| STRING
	;
